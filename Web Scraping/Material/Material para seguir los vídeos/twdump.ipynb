      "source": [
        "import sqlite3\n",
        "\n",
        "conn = sqlite3.connect('spider.sqlite')\n",
        "cur = conn.cursor()\n",
        "cur.execute('SELECT * FROM Twitter')\n",
        "count = 0\n",
        "for row in cur:\n",
        "    print(row)\n",
        "    count = count + 1\n",
        "print(count, 'rows.')\n",
        "cur.close()\n"
